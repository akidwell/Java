import java.util.Scanner;

public class StudentRegistrationApp {

	public static void main(String[] args) {
		System.out.println("Welcome to the Student Registration");
		
		//Scanner
		Scanner sc = new Scanner(System.in);
		
		//Prompt User input
		String firstName1 = " ";
		System.out.print("Enter First Name:  \n");
		String lastName1 = " ";
		System.out.print("Enter Last Name:   \n");
		int birthYear1 = 0;
		System.out.print("Enter 4-digit birth year:  \n");
	

		
		//biz logic
		 
		String firstName = sc.next();
		
		String lastName = sc.next();
		
		int birthYear = sc.nextInt();
		
		String tempPassword = lastName1 + "*" + birthYear1;
		
		//display results
		System.out.println("Welcome Amber Kidwell");
		System.out.println("Your Registration is Complete");
		System.out.println("Your temp password is:" + tempPassword);
		
		
		//bye
		System.out.println();
		System.out.println("Bye");

		
		
		
		
	}

}
