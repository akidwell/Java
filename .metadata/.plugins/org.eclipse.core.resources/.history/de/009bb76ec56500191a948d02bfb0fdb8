
public class CalculatorProjectApp {

	public static void main(String[] args) {
		
		System.out.println("Calculator App");
		
	
		int j = 7; 
		int k = 8;
		int result1 = j + k;
		System.out.println("J= 7 and k= 8");
		System.out.println("j + k = ");
		System.out.println(j + k);
		System.out.println();
		
		int j = 7; 
		int k = 8;
		int result1 = j - k;
		System.out.println("J= 7 and k=8");
		System.out.println("j - k = ");
		System.out.println();
		System.out.println();

		
		
		
		

	}

}
